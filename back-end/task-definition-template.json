{
  "family": "${AWSECS_FAMILY}",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::050107530013:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "${AWSECS_CONTAINER}",
      "image": "${AWS_ECR_REGISTRY_URL}",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "${AWSLOG_GROUP}",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "1024"
}





